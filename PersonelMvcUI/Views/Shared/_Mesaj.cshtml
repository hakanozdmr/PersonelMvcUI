@model PersonelMvcUI.ViewModels.MesajViewModel

@if (Model.Status == true)
{
    <div class="alert alert-success">
        @Model.Mesaj <a href="@Model.Url"> @Model.LinkText</a>
    </div>
}
else
{
    <div class="alert alert-danger">
        @Model.Mesaj <a href="@Model.Url"> @Model.LinkText</a>
    </div>
}
